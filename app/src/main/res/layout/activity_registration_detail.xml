<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical">

    <android.support.v4.widget.NestedScrollView
        android:id="@+id/nested"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbarSize="0dp">

        <RelativeLayout
            android:id="@+id/pic"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#fff"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:scaleType="fitXY"
                android:src="@drawable/pharmacyimg2" />

            <ImageView
                android:id="@+id/imgback"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:paddingLeft="16dp"
                android:src="@drawable/ic_back_arrow"
                android:onClick="onBackRegistrationClick"/>

            <RelativeLayout
                android:id="@+id/relative"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/imageView"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/profile"
                    android:layout_width="52dp"
                    android:layout_height="52dp"
                    android:layout_gravity="center"
                    android:layout_margin="22dp"
                    android:maxHeight="48dp"
                    android:src="@drawable/img_doctor"
                    android:tag="image_tag" />

                <LinearLayout
                    android:id="@+id/linearName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="20dp"
                    android:layout_toRightOf="@id/profile"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/tvName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Cherry Pharmacy"
                        android:textStyle="bold"
                        android:textColor="#000000"
                        android:textSize="16dp"/>

                    <TextView
                        android:id="@+id/tvTown"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/margin_md"
                        android:layout_toRightOf="@+id/profile"
                        android:text="Town"
                        android:textSize="14dp" />

                </LinearLayout>

                <ImageView
                    android:id="@+id/phone"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_below="@+id/imageView"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/margin_lg"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="30dp"
                    android:background="@drawable/ic_phone_black_24dp"
                    android:scaleType="fitXY"
                    tools:ignore="NotSibling" />

            </RelativeLayout>

            <View
                android:id="@+id/view"
                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:layout_below="@+id/relative"
                android:layout_marginTop="@dimen/margin_sm"
                android:layout_marginBottom="10dp"
                android:background="#F7F5F5" />

            <LinearLayout
                android:id="@+id/linear1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/view"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/home"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:src="@drawable/addressimg"
                        android:layout_marginLeft="@dimen/default_margin"
                        android:layout_marginTop="@dimen/margin_md"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/margin_md"
                        android:layout_toRightOf="@+id/home"
                        android:orientation="vertical"
                        tools:ignore="NotSibling">

                        <TextView
                            android:id="@+id/address"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Address"
                            android:layout_marginLeft="@dimen/margin_md"
                            android:textStyle="bold"
                            style="@style/NormalTextInputLayout"
                            tools:ignore="DuplicateIds" />

                        <TextView
                            android:id="@+id/tvaddress"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_sm"
                            android:text=""
                            android:layout_marginLeft="@dimen/margin_md"
                            style="@style/NormalTextInputLayout"
                            tools:ignore="DuplicateIds" />
                    </LinearLayout>
                </RelativeLayout>

                <View
                    android:id="@+id/view1"
                    android:layout_width="wrap_content"
                    android:layout_height="1dp"
                    android:layout_below="@+id/linear1"
                    android:layout_marginTop="@dimen/margin_md"
                    android:layout_marginBottom="10dp"
                    android:background="#F7F5F5"
                    tools:ignore="NotSibling" />

                <LinearLayout
                    android:id="@+id/linear2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/view1"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imgabout"
                            android:layout_width="18dp"
                            android:layout_height="18dp"
                            android:src="@drawable/aboutimag4"
                            android:layout_marginLeft="@dimen/default_margin"
                            android:layout_marginTop="@dimen/margin_md"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_md"
                            android:layout_toRightOf="@+id/imgabout"
                            android:orientation="vertical"
                            tools:ignore="NotSibling">

                            <TextView
                                android:id="@+id/about"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="About"
                                android:textStyle="bold"
                                android:layout_marginLeft="@dimen/margin_md"
                                style="@style/NormalTextInputLayout"
                                tools:ignore="DuplicateIds" />

                            <TextView
                                android:id="@+id/tvabout"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/margin_sm"
                                android:text=""
                                android:layout_marginLeft="@dimen/margin_md"
                                style="@style/NormalTextInputLayout"
                                tools:ignore="DuplicateIds" />
                        </LinearLayout>

                    </RelativeLayout>

                    <View
                        android:id="@+id/view2"
                        android:layout_width="wrap_content"
                        android:layout_height="1dp"
                        android:layout_below="@+id/linear2"
                        android:layout_marginTop="@dimen/margin_md"
                        android:layout_marginBottom="10dp"
                        android:background="#F7F5F5"
                        tools:ignore="NotSibling" />

                    <LinearLayout
                        android:id="@+id/linear3"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@+id/view2"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/time"
                                android:layout_width="18dp"
                                android:layout_height="18dp"
                                android:src="@drawable/clock1"
                                android:layout_marginLeft="@dimen/default_margin"
                                android:layout_marginTop="@dimen/margin_md"/>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="@dimen/margin_md"
                                android:layout_toRightOf="@+id/time"
                                android:orientation="vertical"
                                tools:ignore="NotSibling">

                                <TextView
                                    android:id="@+id/texttime"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Available Time"
                                    android:textStyle="bold"
                                    android:layout_marginLeft="@dimen/margin_md"
                                    style="@style/NormalTextInputLayout"
                                    tools:ignore="DuplicateIds" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:layout_below="@id/texttime" >

                                    <TextView
                                        android:id="@+id/starttime"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/margin_sm"
                                        android:text="Start Time :"
                                        android:layout_marginLeft="@dimen/margin_md"
                                        android:textColor="@color/black"
                                        android:textSize="12sp"
                                        tools:ignore="DuplicateIds" />

                                    <TextView
                                        android:id="@+id/tvstarttime"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/margin_sm"
                                        android:layout_toRightOf="@id/starttime"
                                        android:text=" "
                                        android:layout_marginLeft="@dimen/margin_md"
                                        android:textSize="12sp"
                                        android:textColor="@color/black"
                                        tools:ignore="DuplicateIds" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:layout_below="@id/texttime" >

                                    <TextView
                                        android:id="@+id/endtime"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/margin_sm"
                                        android:text="End Time   :"
                                        android:layout_marginLeft="@dimen/margin_md"
                                        android:textSize="12sp"
                                        android:textColor="@color/black"
                                        tools:ignore="DuplicateIds" />

                                    <TextView
                                        android:id="@+id/tvendtime"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/margin_sm"
                                        android:layout_toRightOf="@id/starttime"
                                        android:text=" "
                                        android:layout_marginLeft="@dimen/margin_md"
                                        android:textSize="12sp"
                                        android:textColor="@color/black"
                                        tools:ignore="DuplicateIds" />

                                </LinearLayout>
                            </LinearLayout>

                        </RelativeLayout>

                        <View
                            android:id="@+id/view3"
                            android:layout_width="wrap_content"
                            android:layout_height="1dp"
                            android:layout_below="@+id/linear3"
                            android:layout_marginTop="@dimen/margin_md"
                            android:layout_marginBottom="10dp"
                            android:background="#F7F5F5"
                            tools:ignore="NotSibling" />

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

        </RelativeLayout>

    </android.support.v4.widget.NestedScrollView>
</RelativeLayout>

